{
  "name": "cultuurnet/udb3",
  "description": "UDB3 PHP libraries",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Publiq vwz",
      "email": "info@publiq.be"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-tidy": "*",
    "2dotstwice/collection": "~1.1",
    "broadway/broadway": "~0.10",
    "cakephp/chronos": "^1.1",
    "chrisboulton/php-resque": "~1.2",
    "commerceguys/intl": "^0.7.1",
    "cultuurnet/broadway-tools": "~0.1",
    "cultuurnet/cdb": "~2.2.0",
    "cultuurnet/culturefeed-php": "~1.7",
    "cultuurnet/deserializer": "~0.1",
    "cultuurnet/geocoding": "~0.1",
    "cultuurnet/udb3-models": "^2.0",
    "cultuurnet/valueobjects": "~3.0",
    "danielstjules/stringy": "~1.9",
    "doctrine/dbal": "~2.4",
    "guzzlehttp/psr7": "^1.2",
    "justinrainbow/json-schema": "1.4.3",
    "league/flysystem": "~1.0",
    "league/period": "^3.3",
    "php-http/httplug": "^1.0",
    "predis/predis": "~1.0",
    "psr/log": "~1.0",
    "ramsey/uuid": "^2.9"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "~2.0",
    "mikey179/vfsstream": "~1.6.2",
    "phing/phing": "~2.10",
    "phpstan/phpstan": "^0.12.42",
    "phpunit/phpunit": "^7.5",
    "satooshi/php-coveralls": "~0.7",
    "squizlabs/php_codesniffer": "2.6.1"
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\UDB3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultuurNet\\UDB3\\": "test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "phing": "./vendor/bin/phing test"
  }
}

  "require": {
    "cultuurnet/calendar-summary": "~1.0.2",
    "cultuurnet/udb2-domain-events": "dev-III-197 as 0.1",
    "gridonic/princexml-php": "~1.1.1",
    "rase/socket.io-emitter": "0.6.1",
    "monolog/monolog": "~1.11",
    "psr/log": "~1.0",
    "cultuurnet/search": "~1.1",
    "cultuurnet/entry": "~0.0@dev",
    "cultuurnet/cdb": "~2.1",
    "cultuurnet/deserializer": "~0.1",
    "broadway/broadway": "~0.2.0",
    "chrisboulton/php-resque": "~1.2",
    "cultuurnet/culturefeed-php": "~1.5",
    "nicolopignatelli/valueobjects": "~2.0",
    "php": ">=5.5.0",
    "phpoffice/phpexcel": "~1.8",
    "ext-tidy": "*",
    "twig/twig": "~1.0",
    "league/flysystem": "~1.0",
    "league/flysystem-ziparchive": "~1.0",
    "danielstjules/stringy": "~1.9",
    "doctrine/dbal": "~2.4"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0",
    "phpunit/phpunit": "~4.3",
    "squizlabs/php_codesniffer": "~2.2",
    "phing/phing": "~2.10",
    "alchemy/zippy": "~0.2",
    "satooshi/php-coveralls": "~0.7"
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\UDB3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultuurNet\\UDB3\\": "test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "provide" : {
    "ext-redis": "*"
  }
}
